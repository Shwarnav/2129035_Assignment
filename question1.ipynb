   "source": [
    "#Q1.Find the approximate change in the value of 1/x when x changes from 2 to 100\n",
    "import numpy as np\n",
    "import pandas as pd \n",
    "import sympy as sym"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/latex": [
       "$\\displaystyle - \\frac{49}{2}$"
      ],
      "text/plain": [
       "-49/2"
      ]
     },
     "execution_count": 18,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "x,y=sym.symbols('x y')\n",
    "y=sym.diff(1/x**2,x)*(100-2)\n",
    "y.doit().subs({x:2})"
   ]
  }
